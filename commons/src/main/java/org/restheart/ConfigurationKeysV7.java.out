package org.restheart;

/**
 *
 * @author Andrea Di Cesare {@literal <andrea@softinstigate.com>}
 */
public interface ConfigurationKeysV7 {
    /**
     * the key for http listener.
     */
    public static final String HTTP_LISTENER_KEY = "http-listener";

    /**
     * the key for https listener.
     */
    public static final String HTTPS_LISTENER_KEY = "https-listener";

    /**
     * the key for ajp listener.
     */
    public static final String AJP_LISTENER_KEY = "ajp-listener";

    /**
     * the key for logging.
     */
    public static final String LOGGING_KEY = "logging";

    /**
     * the key for static resources.
     */
    public static final String STATIC_RESOURCES_KEY = "static-resources";

    /**
     * the key for proxies.
     */
    public static final String PROXIES_KEY = "proxies";

    /**
     * the key for core.
     */
    public static final String CORE_KEY = "core";

    /**
     * the key for connection options.
     */
    public static final String CONNECTION_OPTIONS_KEY = "connection-options";
}
